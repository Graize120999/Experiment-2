// Create a program that will generate the multiplication table (up to x10) of a number.

#include <iostream>
#include <cmath>
#include <conio.h>

using namespace std; 

int main ()
{
	int a;
		cout << "Enter A Number: ";
		cin >> a; 
		
		cout << "\n";
		
	for (int i = 1; i<=10; i++)
	{
		cout << a << " x " << i << " = " << (a*i);
		cout << "\n";
	}
		 	
	_getch ();
	return 0;
}
